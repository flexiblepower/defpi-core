{
	"name": "Dashboard",
	"version": "1",
	"parameters": [
		{
			"id": "cemHashPair",
			"name": "cemHashPair",
			"type": "string"
		},
		{
			"id": "rmHashPair",
			"name": "rmHashPair",
			"type": "string"
		},
		{
			"id": "obsConsumerHashPair",
			"name": "obsConsumerHashPair",
			"type": "string"
		},
		{
			"id": "obsPublisherHashPair",
			"name": "obsPublisherHashPair",
			"type": "string"
		}
	],
	"interfaces": [
		{
			"name": "gateway",
			"autoConnect": true,
			"allowMultiple": false,
			"interfaceVersions": [
				{
					"versionName": "HTTP",
					"type": "proto",
