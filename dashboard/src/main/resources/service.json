{
	"name": "Dashboard",
	"version": "1",
	"parameters": [],
	"interfaces": [
		{
			"name": "gateway",
			"autoConnect": true,
			"allowMultiple": false,
			"interfaceVersions": [
				{
					"versionName": "HTTP",
					"type": "proto",
					"location": "https://raw.githubusercontent.com/defpi/interfaces/master/defpi/HTTP.proto",
					"sends": [
						"HTTPResponse"
					],
					"receives": [
						"HTTPRequest"
					]
				}
			]
		},
		{
			"name": "widget",
			"autoConnect": false,
			"allowMultiple": true,
			"interfaceVersions": [
				{
					"versionName": "HTTP",
					"type": "proto",
					"location": "https://raw.githubusercontent.com/defpi/interfaces/master/defpi/Widget.proto",
					"sends": [
						"WidgetHTTPRequest"
					],
					"receives": [
						"WidgetHTTPResponse"
					]
				}
			]
		}
	]
}
