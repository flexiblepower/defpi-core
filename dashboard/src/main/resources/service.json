			]
		},
		{
			"name": "widget",
			"autoConnect": true,
			"allowMultiple": true,
			"interfaceVersions": [
				{
					"versionName": "HTTP",
					"type": "proto",
					"location": "https://raw.githubusercontent.com/defpi/interfaces/master/defpi/Widget.proto",
					"sends": [
						"WidgetHTTPRequest"
					],
					"receives": [
						"WidgetInfo",
						"WidgetHTTPResponse"
					]
				}
			]
		},
	    {
			"name": "Observation publisher",
			"autoConnect": false,
			"allowMultiple": true,
			"interfaceVersions": [
				{
					"versionName": "1",
					"type": "proto",
					"location": "https://raw.githubusercontent.com/defpi/interfaces/master/defpi-ext/Observation.proto",
					"sends": [
						"Observation"
					],
					"receives": []
				}
			]
		}
