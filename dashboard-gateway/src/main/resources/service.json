{
	"name": "Dashboard Gateway",
	"version": "1",
	"interfaces": [
		{
			"name": "Dashboard",
			"autoConnect": true,
			"allowMultiple": true,
			"interfaceVersions": [
				{
					"versionName": "HTTP",
					"type": "proto",
					"location": "https://raw.githubusercontent.com/defpi/interfaces/6098df232adb24fe17612857ecc23597d5174680/defpi/HTTP.proto",
					"sends": [
						"HTTPRequest"
					],
					"receives": [
						"HTTPResponse"
					]
				}
			]
		}
	]
}
