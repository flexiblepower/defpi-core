{
	"name": "Dashboard Gateway",
	"version": "1",
	"description": "This is the official FAN dashboard gateway. This service serves as the single point for all users in the dEF-Pi cluster to connect to their own dashboard",
	"iconURL": "https://upload.wikimedia.org/wikipedia/commons/3/37/Adobe_ConnectNow_icon.png",
	"interfaces": [
		{
			"name": "Dashboard",
			"autoConnect": true,
			"allowMultiple": true,
			"interfaceVersions": [
				{
					"versionName": "HTTP",
					"type": "proto",
					"location": "https://raw.githubusercontent.com/defpi/interfaces/6098df232adb24fe17612857ecc23597d5174680/defpi/HTTP.proto",
					"sends": [
						"HTTPRequest"
					],
					"receives": [
						"HTTPResponse"
					]
				}
			]
		}
	]
}
