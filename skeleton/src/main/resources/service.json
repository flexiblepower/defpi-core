{
  "name": "PV Panel Simulation",
  "version": "1.0-skeleton",
  "interfaces": [
    {
      "name": "widget",
      "autoConnect": false,
      "allowMultiple": false,
      "interfaceVersions": [
        {
          "versionName": "http",
          "type": "proto",
          "location": "HTTP.proto",
          "receives": [ "HTTPRequest" ],
          "sends": [ "HTTPResponse" ]
        }
      ]
    },
    {
      "name": "efi",
      "autoConnect": false,
      "allowMultiple": false,
      "interfaceVersions": [
        {
          "versionName": "efi 2.0",
          "type": "xsd",
          "location": "https://raw.githubusercontent.com/flexiblepower/efi/master/schema/efi-2.0.xsd",
          "sends": [
            "InflexibleInstruction",
            "InstructionRevoke"
          ],
          "receives": [
            "InflexibleRegistration",
            "InflexibleCurtailmentOptions",
            "InflexibleForecast",
            "Measurement",
            "InstructionStatusUpdate",
            "FlexibilityRevoke"
          ]
        }
      ]
    }
  ]
}